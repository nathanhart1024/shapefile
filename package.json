{
  "name": "shapefile",
  "version": "0.4.0",
  "description": "An implementation of the shapefile (.shp) spatial data format.",
  "keywords": [
    "geojson",
    "shapefile"
  ],
  "homepage": "https://github.com/mbostock/shapefile",
  "license": "BSD-3-Clause",
  "author": {
    "name": "Mike Bostock",
    "url": "https://bost.ocks.org/mike"
  },
  "main": "dist/shapefile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mbostock/shapefile.git"
  },
  "bin": {
    "dbfcat": "./bin/dbfcat",
    "shpcat": "./bin/shpcat",
    "shp2json": "./bin/shp2json"
  },
  "scripts": {
    "test": "tape 'test/**/*-test.js'",
    "prepublish": "rm -rf dist && mkdir dist && rollup --banner \"$(preamble)\" -f umd -n shapefile -o dist/shapefile.js -- dbf/index.js && uglifyjs --preamble \"$(preamble)\" -o dist/shapefile.min.js -cm -- dist/shapefile.js",
    "postpublish": "git push && git push --tags"
  },
  "dependencies": {
    "commander": "2",
    "path-source": "0.0",
    "text-encoding": "0.6"
  },
  "devDependencies": {
    "package-preamble": "0.0",
    "rollup": "0.34",
    "tape": "4",
    "uglify-js": "2"
  }
}
